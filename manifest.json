{
  "manifest_version": 3,
  "name": "Static Page Downloader",
  "version": "1.0",
  "description": "Inlines resources and downloads a static version of the current page.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_title": "Download Static Page",
    "default_icon": {
      "128": "icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
